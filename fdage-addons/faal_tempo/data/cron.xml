<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="import_csv_file" model="ir.cron">
            <field name="name">Import des sale order via les fichiers CSV</field>
            <field name="model_id" ref="model_sale_order"/>
            <field name="state">code</field>
            <field name="code">model._import_csv_files()</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>

        <record id="export_csv_file" model="ir.cron">
            <field name="name">Export des donn√©es de la base pour swing</field>
            <field name="model_id" ref="model_sale_order"/>
            <field name="state">code</field>
            <field name="code">model._export_csv_files()</field>
            <field name="interval_number">2</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="False"/>
        </record>

        <record id="recouncil_after_import" model="ir.cron">
            <field name="name">Lettrage manquant pour les comptes</field>
            <field name="model_id" ref="model_account_account"/>
            <field name="state">code</field>
            <field name="code">model._recouncil_bad_import()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">0</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="False"/>
        </record>

    </data>


</odoo>

